[
  {
    "resource": {
      "aws_s3_bucket": {
        "test": {
          "bucket": "pretf-test-dev",
          "acl": "private",
          "server_side_encryption_configuration": {
            "rule": {
              "apply_server_side_encryption_by_default": {
                "sse_algorithm": "AES256"
              }
            }
          }
        }
      }
    }
  },
  {
    "data": {
      "aws_iam_policy_document": {
        "lambda": {
          "statement": {
            "effect": "Allow",
            "actions": [
              "s3:ListBucket",
              "s3:PutObject"
            ],
            "resources": [
              "${aws_s3_bucket.test.arn}",
              "${aws_s3_bucket.test.arn}/*"
            ]
          }
        }
      }
    }
  }
]